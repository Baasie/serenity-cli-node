{
  "name": "serenity-aggregator",
  "version": "0.0.1",
  "description": "Generates HTML reports from Serenity BDD-compatible JSON files",
  "scripts": {
    "test": "node ./node_modules/gulp/bin/gulp.js test",
    "prepublish": "node ./node_modules/gulp/bin/gulp.js prepublish",
    "postinstall": "npm run typings -- install",
    "typings": "typings"
  },
  "keywords": [
    "tdd",
    "bdd",
    "test",
    "testing",
    "serenity",
    "reporting",
    "documentation"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/smartcodeltd/serenity-aggregator.git"
  },
  "bugs": {
    "url": "https://github.com/smartcodeltd/serenity-aggregator/issues"
  },
  "bin": {
    "serenity": "bin/serenity"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "author": "Jan Molak <jan.molak@smartcodeltd.co.uk>",
  "homepage": "https://github.com/smartcodeltd/serenity-aggregator",
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "chai": "3.5.0",
    "chai-as-promised": "5.3.0",
    "gulp": "3.9.1",
    "gulp-clean": "0.3.2",
    "gulp-concat": "2.6.0",
    "gulp-istanbul": "1.0.0",
    "gulp-mocha": "3.0.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-tslint": "6.0.2",
    "gulp-typescript": "2.13.6",
    "merge2": "1.0.2",
    "mocha": "3.0.0",
    "remap-istanbul": "0.6.4",
    "run-sequence": "1.2.2",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "ts-helper": "0.0.1",
    "ts-node": "1.2.2",
    "tslint": "3.14.0",
    "typescript": "1.8.10",
    "typings": "1.3.2"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 2"
  }
